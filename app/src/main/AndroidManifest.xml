<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthcare">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WebShishu"></activity>
        <activity android:name=".WebSquare" />
        <activity android:name=".WebCentral" />
        <activity android:name=".WebUnited" />
        <activity android:name=".WebApollo" />
        <activity android:name=".DetailDiarrhea" />
        <activity android:name=".DetailGastric" />
        <activity android:name=".DetailAllergy" />
        <activity android:name=".DetailFever" />
        <activity android:name=".HospitalActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".PillsActivity" />
        <activity android:name=".AmbulanceActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>